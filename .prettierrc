{
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "auto",
    "bracketSpacing": false,
    "overrides": [
      {
        "files": "*.{less,sass,scss,css,pcss}",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
  